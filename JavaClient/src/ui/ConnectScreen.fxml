<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="261.0" prefWidth="326.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ui.ConnectScreenController">
    <stylesheets>
        <URL value="@connectscreen.css" />
    </stylesheets>
    <children>
        <Button fx:id="btnConnect" layoutX="207.0" layoutY="200.0" mnemonicParsing="false" onAction="#handleConnectAction" prefHeight="51.0" prefWidth="105.0" text="Connect">
            <font>
                <Font name="Monospaced Regular" size="12.0" />
            </font>
        </Button>
        <Label layoutX="105.0" layoutY="38.0" prefHeight="34.0" prefWidth="118.0" text="P2P Chat" textAlignment="CENTER">
            <font>
                <Font name="Monospaced Regular" size="22.0" />
            </font>
        </Label>
        <TextField fx:id="txtCodename" layoutX="73.0" layoutY="98.0" prefHeight="26.0" prefWidth="180.0" promptText="Codename">
            <font>
                <Font name="Monospaced Regular" size="14.0" />
            </font>
        </TextField>
        <Label layoutX="6.0" layoutY="226.0" prefHeight="34.0" prefWidth="91.0" text="With &lt;3 by PSG">
            <font>
                <Font name="Monospaced Regular" size="10.0" />
            </font>
        </Label>
        <TextField fx:id="txtIp" layoutX="73.0" layoutY="138.0" prefHeight="26.0" prefWidth="180.0" promptText="ip address">
            <font>
                <Font name="Monospaced Regular" size="14.0" />
            </font>
        </TextField>
        <Label fx:id="myIp" layoutX="73.0" layoutY="172.0" prefHeight="15.0" prefWidth="180.0" text="My ip:">
            <font>
                <Font name="Monospaced Regular" size="12.0" />
            </font>
        </Label>
    </children>
</AnchorPane>
